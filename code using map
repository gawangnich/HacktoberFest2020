#include<bits/stdc++.h> 
// g++ hello.cpp && ./a.out
using namespace std;

int main () {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	// #ifdef DEBUG
	// freopen("input.txt", "rt", stdin);
	// #endif
	int T;
	cin >> T;
	int ans = 0;
	while(T--) {
		int n;
		cin >> n;
		int arr[n];
		map<int,int> mp;
		for (int i = 0; i < n; i++) {
			cin >> arr[i];
		}
		for (int i = 0; i < n; i++) {
			if(arr[i] != 0) {
				mp[arr[i]]++;
			}	
		}
		ans = mp.size();
		cout << ans << endl;
	}
	return 0;
}
